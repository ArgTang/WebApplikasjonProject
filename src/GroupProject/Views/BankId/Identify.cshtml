@{
    Layout = "shared/_bankid";
    ViewBag.bankIDTitle = "Identifisering";
    ViewBag.footer = true;
    ViewBag.footerTitle = "BankID brukersted";
    ViewBag.footerContent = "ACOS Sparebank";
}

<main class="body" role="main">
    <div class="viewport animate">
        <div class="lm_view padding block_vertical_center">
            <div>
                <form id="__AjaxAntiForgeryForm" method="post" role="form" class="form_wrapper" autocomplete="off">
                        <div class="row label">
                            <label>Fødselsnummer</label>
                        </div>
                        <div class="row form">
                            <div class="input_wrapper">
                                <div class="infobubble_wrapper hide-error" data-position="top">
                                    <div class="infobubble_container">
                                        <div class="infobubble">
                                            <div class="arrow">
                                                <div class="border"></div>
                                                <div class="fill"></div>
                                            </div>
                                            <div class="wrapper">
                                                <img class="icon" src="data:image/svg+xml;base64,">
                                                <span class="text">Fødselsnummer må være 11 siffer</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="label">
                                    <div class="icon">
                                        <div>
                                            <img alt=""
                                                 src="data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMjAiIGhlaWdodD0iMjIiIHhtbG5zPSJodHRwOi8vd3d3LnczLm9yZy8yMDAwL3N2ZyI+PHRpdGxlPmljb191c2VyaWQ8L3RpdGxlPjxnIGZpbGw9IiMwMDc4QkQiPjxwYXRoIGQ9Ik0yMCAyMmMwLTUuNTIzLTQuNDc3LTEwLTEwLTEwcy0xMCA0LjQ3Ny0xMCAxMGgyMHoiLz48Y2lyY2xlIGN4PSIxMCIgY3k9IjUiIHI9IjUiLz48L2c+PC9zdmc+"
                                                 title="">
                                        </div>
                                    </div>
                                    <div class="input">
                                        <div class="wrp">
                                            @Html.AntiForgeryToken()
                                            <input id="birthnumberInput" class="">
                                            <div class="placeholder"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="button_icon_wrapper">
                                <button id="submitButton" class="button" title="Neste" onclick="return false">
                                    <span class="label" style="display: none;"></span>
                                    <img alt="" class="svg"
                                         src="data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iMjQiIGhlaWdodD0iMTkiIHhtbG5zPSJodHRwOi8vd3d3LnczLm9yZy8yMDAwL3N2ZyI+PHRpdGxlPmljb19hcnJvd19yaWdodDwvdGl0bGU+PHBhdGggZD0iTTE5IDExbC01IDUgMiAyIDgtOC41MzEtOC04LjQ2OS0yIDIgNSA1aC0xOXYzaDE5eiIgZmlsbD0iI2ZmZiIvPjwvc3ZnPg==">
                                </button>
                            </div>
                        </div>
                        <div class="row message">
                            <span>11 siffer</span>
                        </div>
                    <div class="text-danger"></div>
                    </form>
            </div>
        </div>
    </div>
</main>
